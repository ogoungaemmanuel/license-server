{
    "name": "xslain/license-server",
    "description": "License server for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "license",
        "license-server",
        "license-manager"
    ],
    "authors": [
        {
            "name": "EgoistDeveloper",
            "email": "epsilonelittr@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/xslain/license-server/issues",
        "source": "https://github.com/xslain/license-server"
    },
    "require": {
        "php": "^8.2 || ^8.1 || ^8.0",
        "ext-filter": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "illuminate/support": "^11.0 || ^10.0 || ^9.0",
        "jeremykendall/php-domain-parser": "^6.3.0",
        "xslain/ultimate-support": "^2.0 || ^1.1",
        "laravel/sanctum": "^4.0 || ^3.2 || ^3.0 || ^2.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^7.42",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Xslain\\LicenseServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xslain\\LicenseServer\\LicenseServerServiceProvider"
            ],
            "aliases": {
                "LicenseServer": "Xslain\\LicenseServer\\Facades\\LicenseServer"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
